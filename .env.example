# Copia este archivo como .env.local y completa con tus valores reales

# Clave API de Gemini (obtén una en https://makersuite.google.com/app/apikey)
GEMINI_API_KEY=tu_clave_api_aqui

# Clave API de Google Places (obtén una en https://console.cloud.google.com/)
VITE_GOOGLE_PLACES_API_KEY=tu_google_places_api_key

# Configuración de Firebase (obtén estos valores en la consola de Firebase)
VITE_FIREBASE_API_KEY=tu_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=tu_proyecto.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=tu_proyecto_id
VITE_FIREBASE_STORAGE_BUCKET=tu_proyecto.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=tu_messaging_sender_id
VITE_FIREBASE_APP_ID=tu_app_id